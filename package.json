{
  "name": "mhbo-js",
  "version": "1.0.0",
  "description": "A JS wrapper for interacting with the MHBO API.",
  "main": "lib/index.js",
  "repository": "https://github.com/mhbo/mhbo-js.git",
  "author": "MHBO Inc.",
  "license": "MIT",
  "private": true,
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "flow-bin": "^0.72.0",
    "flow-copy-source": "^1.3.0",
    "flow-remove-types": "^1.2.3",
    "jest": "^23.6.0",
    "prettier": "^1.12.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "flow": "flow",
    "test": "jest --config jestconfig.json",
    "build": "rm -rf ./lib; tsc",
    "build2": " flow-copy-source src lib; flow-remove-types src/ -d lib/; prettier --write ./lib/*.js ./lib/**/*.js",
    "release": "yarn run build; find ./lib -type f -name \"*.test.js\" -delete; find ./lib -type f -name \"*.flow.js\" -delete",
    "prepublish": "yarn run release"
  },
  "dependencies": {
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.118",
    "@types/lodash.snakecase": "^4.1.4",
    "es6-promise": "^4.2.4",
    "flow": "^0.2.3",
    "global": "^4.3.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.11",
    "lodash.snakecase": "^4.1.1",
    "qs": "^6.5.2"
  }
}
